<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <description></description>
    <display-name>FrontController</display-name>
    <servlet-name>FrontController</servlet-name>
    <servlet-class>com.revature.servlets.FrontController</servlet-class>
    <init-param>
      <param-name>DefaultRole</param-name>
      <param-value>User</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>FrontController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmployeeAddReimbursementServlet</display-name>
    <servlet-name>EmployeeAddReimbursementServlet</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeAddReimbursementServlet</servlet-class>
    <init-param>
      <param-name>DefaultRole</param-name>
      <param-value>Employee</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeAddReimbursementServlet</servlet-name>
    <url-pattern>/reimbursementRequest</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmployeeViewAllPastTicketsServlet</display-name>
    <servlet-name>EmployeeViewAllPastTicketsServlet</servlet-name>
    <servlet-class>com.revature.servlets.EmployeeViewAllPastTicketsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmployeeViewAllPastTicketsServlet</servlet-name>
    <url-pattern>/EmployeeViewAllPastTicketsServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>ViewAllPastTicketsController</display-name>
    <servlet-name>ViewAllPastTicketsController</servlet-name>
    <servlet-class>com.revature.controllers.ViewAllPastTicketsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewAllPastTicketsController</servlet-name>
    <url-pattern>/ViewAllPastTicketsController</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <description></description>
    <display-name>ManagerViewAllTicketsController</display-name>
    <servlet-name>ManagerViewAllTicketsController</servlet-name>
    <servlet-class>com.revature.controllers.ManagerViewAllTicketsController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManagerViewAllTicketsController</servlet-name>
    <url-pattern>/ManagerViewAllTicketsController</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>ManagerViewAllTicketsServlet</display-name>
    <servlet-name>ManagerViewAllTicketsServlet</servlet-name>
    <servlet-class>com.revature.servlets.ManagerViewAllTicketsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ManagerViewAllTicketsServlet</servlet-name>
    <url-pattern>/ManagerViewAllTicketsServlet</url-pattern>
  </servlet-mapping>
  
  
<filter>
     <filter-name>CORS</filter-name>
     <filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
     <init-param>
         <param-name>cors.allowed.origins</param-name>
         <param-value>*</param-value>
     </init-param>
     <init-param>
         <param-name>cors.allowed.methods</param-name>
         <param-value>GET, POST, HEAD, OPTIONS, PUT, DELETE</param-value>
     </init-param>
 </filter>

<filter-mapping>
     <filter-name>CORS</filter-name>
     <url-pattern>/*</url-pattern>
 </filter-mapping>
</web-app>